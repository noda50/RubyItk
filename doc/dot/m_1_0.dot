digraph TopLevel {
    label = "TopLevel"
    fontname = Arial
    bgcolor = lightcyan1
    fontsize = 8
    compound = true
    node [
        color = black,
        fontname = Arial,
        fontsize = 8
    ]

    subgraph cluster_XML {
        color = blue
        label = "XML"
        fontname = Arial
        URL = "classes/XML.html"
        fillcolor = palegreen1
        style = filled
        XML [
            width = 0.75,
            shape = plaintext,
            height = 0.01,
            label = ""
        ]

    }

    REXML [
        shape = box,
        URL = "classes/REXML.html",
        label = "REXML"
    ]

    REXML -> XML [
        lhead = cluster_XML
    ]

}

